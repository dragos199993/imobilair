// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model EarlyAccessSubmissions {
  id          String     @id @default(uuid())
  created_at  DateTime
  name        String
  email       String
}

model Listing {
  id        String     @id @default(uuid())
  title     String
  content   String?
  userId  String
}
